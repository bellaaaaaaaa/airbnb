<h1>Your Listings</h1>
  <% @my_listings.each do |listing| %>
    <tr>
      <td><%= listing.name %></td><br>
      <td><%= listing.roomtype %></td><br>
      <td><%= listing.price_per_night.to_s %></td><br>
      <td><%= link_to 'Edit', edit_listing_path(listing) %></td> |
      <td><%= link_to 'Show', listing_path(listing) %></td> |
      <td><%= link_to 'Destroy', listing_path(listing),
              method: :delete,
              data: { confirm: 'Are you sure?' } %></td><br>
    </tr><br>
  <% end %>
</table>